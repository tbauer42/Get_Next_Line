																																												  }
																																												  
																																												  intget_next_line(const int f		d, char **line)
																																												  {
																																														static char*str;
																																															inti;
																																																char*str_free;
																																																
																																																" Pre						ss ? for help             |  1 /* ************************************************************************** */

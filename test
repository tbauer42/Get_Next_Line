																																																																																			   voidget_norm(char **line, char **str, char **str_free, i	nt i)
																																																																																			   {
																																																																																					(*line) = ft_strsub(*str, 0, i);
																																																																																						*str_free = ft_strdup(*str + i + 1);
																																																																																							free(*str);
																																																																																								*str = ft_strdup(*str_free);
																																																																																									free(*str_free);
																																																																																			   }
																																																																																			   

																																																																																																														   {
																																																																																																																if (fd == -1 || line == NULL || BUFF_SIZE <= 0)
																																																																																																																			return (-1);
																																																																																																																				if (!*str)
																																																																																																																					" Press ? for help             |    1 /* ************************************************************************** */
																																																																																																																						{
																																																																																																																							" Press ? for help             |  1 /* ************************************************************************** */
																																																																																																																									if (!(*str = (char*)malloc(sizeof(char) * (BUFF_SIZE + 1))))
																																																																																																																										" Press ? for help             |  1 #include "get_next_line.h"
																																																																																																																													return (-1);
																																																																																																																														}
																																																																																																																															return (0);

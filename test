																																																																																			   intft_error(char **str, char			**line, int fd)
																																																																																			   {
																																																																																					if (fd == -1 || line == NULL || BUFF_SIZE <= 0)
																																																																																								return (-1);
																																																																																									if (!*str)
																																																																																										" Press ? for help             |    1 /* ************************************************************************** */
																																																																																											{
																																																																																												" Press ? for help             |  1 /* ************************************************************************** */
																																																																																														if (!(*str = (char*)malloc(sizeof(char) * (BUFF_SIZE + 1))))
																																																																																															" Press ? for help             |  1 #include "get_next_line.h"
																																																																																																		return (-1);
																																																																																																			}
																																																																																																				return (0);
																																																																																			   }
																																																																																			   
																																																																																			   char*read_it(char *str, int fd)
																																																																																			   {
																																																																																				   
																																																																																					char*buf;		
																																																																																						char*dr_freeman;
																																																																																							intret;
																																																																																							
																																																																																								if(!(buf = (char*)malloc(sizeof(char)			* (BUFF_SIZE + 1))))
																																																																																											return (NULL);
																																																																																												if (fd == 0)
																																																																																														{
																																																																																																	if ((ret = read(fd, buf, BUFF_SIZE)) > 0)
																																																																																																				{
																																																																																																								buf[ret] = '\0';
																																																																																																											dr_freeman = str;
																																																																																																														str = ft_strjoin(str, buf);
																																																																																																																	free(dr_freeman);
																																																																																																																			}
																																																																																																																				}
																																																																																																																					else
																																																																																																																							{
																																																																																																																										while ((ret = read(fd, buf, BUFF_SIZE)) > 0)
																																																																																																																													{
																																																																																																																																	buf[ret] = '\0';
																																																																																																																																				dr_freeman = str;
																																																																																																																																							str = ft_strjoin(str, buf);
																																																																																																																																										free(dr_freeman);
																																																																																																																																												}
																																																																																																																																													}
																																																																																																																																														if (ret < 0)
																																																																																																																																																	return (NULL);
																																																																																																																																																		return (str);
																																																																																			   }
																																																																																			   
																																																																																			   voidget_norm(char **line, char **str, char **str_free, int i)
																																																																																			   {
																																																																																					(*line) = ft_strsub(*str,	 0, i);
																																																																																						*str_free = ft_strdup(*str + i + 1);
																																																																																							free(*str);
																																																																																								*str = ft_strdup(*str_free);
																																																																																									free(*str_free);
																																																																																			   }
																																																																																			   
																																																																																			   intget_next_line(const int fd, char **line)
																																																																																			   {
																																																																																					static char			*str;
																																																																																						inti;
																																																																																							char*str_free;
																																																																																							
																																																																																								if (ft_error(&str, line, fd) == -1					)
																																																																																											return (-1);
																																																																																												if ((str = read_it(str, fd)) == NULL)
																																																																																															return (-1);
																																																																																																i = 0;
																																																																																																	if (str[i])
																																																																																																			{
																																																																																																						while (str[i] && str[i] != '\n')
																																																																																																										i++;
																																																																																																												get_norm(line, &str, &str_free, i);
																																																																																																														return (1);
																																																																																																															}
																																																																																																																else
																																																																																																																			(*line) = ft_strdup("");
																																																																																																																				return (0);
																																																																																			   }

char*read_it(char *str, int fd)
{
	
		char*buf;
			char*dr			_freeman;
				intret;
				
					if(!(buf = (char*)malloc(sizeof(char) * (BUFF_SI		ZE + 1))))
								return (NULL);
									if (fd == 0)
											{
														if ((ret = read(fd, buf, BUFF_SIZE)) > 0)
																	{
																					buf[ret] = '\0';
																								dr_freeman = str;
																											str = ft_strjoin(str, buf);
																														free(dr_freeman);
																																}
																																	}
																																		else
																																				{
																																							while ((ret = read(fd, buf, BUFF_SIZE)) > 0)
																																										{
																																														buf[ret] = '\0';

																																																																																			   {
																																																																																				   
																																																																																					charbuf		[BUFF_SIZE + 1];
																																																																																						char*dr_freeman;
																																																																																							intret;
																																																																																							
																																																																																								if (fd == 0)
																																																																																										{
																																																																																													if (			(ret = read(fd, buf, BUFF_SIZE)) > 0)
																																																																																																{
																																																																																																				buf[ret] = '\0';
